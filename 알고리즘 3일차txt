1088
import java.util.Scanner;

public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int a =sc.nextInt(); // 정수 입력
        int result=0;
        for (int i=0; i<=a; i++){
            if(i%3==0) continue;
            System.out.print(i+" ");

        }

    }
}

